datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}



model User {
    id          Int         @id @default(autoincrement())
    email       String      @unique
    password    String
    hashedRT    String?      @unique
    createdAt   DateTime    @default(now())
}
